{
  "type": "module",
  "name": "prophyt-indexer",
  "version": "1.0.10",
  "private": true,
  "description": "Indexer for Prophyt events on Sui blockchain",
  "main": "server.ts",
  "scripts": {
    "dev": "npx tsx server.ts",
    "build": "npx tsc",
    "db:setup:dev": "npx prisma migrate dev --name init",
    "db:reset:dev": "npx prisma db push --force-reset",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "indexer": "npx tsx indexer.ts",
    "sync:dry-run": "npx tsx scripts/sync-db-to-blockchain.ts --dry-run",
    "sync:markets": "npx tsx scripts/sync-db-to-blockchain.ts",
    "sync:markets:dry-run": "npx tsx scripts/sync-db-to-blockchain.ts --dry-run --no-resolutions",
    "sync:resolutions": "npx tsx scripts/sync-db-to-blockchain.ts --resolutions --no-markets",
    "backfill:dry-run": "npx tsx scripts/backfill-events.ts --dry-run",
    "backfill:live": "npx tsx scripts/backfill-events.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.46.4"
  },
  "dependencies": {
    "@mysten/sui": "^1.18.0",
    "@prisma/client": "^6.6.0",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "file-type": "^21.1.0",
    "fs": "0.0.1-security",
    "json2csv": "6.0.0-alpha.2",
    "p-limit": "^6.2.0"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
